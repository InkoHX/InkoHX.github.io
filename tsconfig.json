{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM","ESNext"],
    "moduleResolution": "Node",
    "jsx": "react",
    "alwaysStrict": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules/**/*",
    "public/**/*",
    ".cache/**/*"
  ]
}
